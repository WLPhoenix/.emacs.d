import java.util.regex.*;

public class JavaRegexTest {
    public static void main(String[] args) {
	Pattern group_info_regex = Pattern.compile("^\\{\\((.*)\\)\\((.*)\\)(\\[\\(.*\\)\\(.*\\)\\(.*\\)\\]+)\\}$");
	Matcher m1 = group_info_regex.matcher("{(FileSystemCounters)(FileSystemCounters)[(FILE_BYTES_WRITTEN)(FILE_BYTES_WRITTEN)(49114)]}");
	System.out.println(m1.matches());
	System.out.println("Group 1: " + m1.group(1));
	System.out.println("Group 2: " + m1.group(2));
	System.out.println("Group 3: " + m1.group(3));

	Pattern counter_info_regex = Pattern.compile("^\\[\\((.*)\\)\\((.*)\\)\\((.*)\\)\\]$");
	Matcher m2 = counter_info_regex.matcher(m1.group(3).trim());
	System.out.println(m2.matches());
	System.out.println("Group 1: " + m2.group(1));
	System.out.println("Group 2: " + m2.group(2));
	System.out.println("Group 3: " + m2.group(3));
    }
}

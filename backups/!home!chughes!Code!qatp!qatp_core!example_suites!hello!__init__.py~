from . import chance_tasks, chance_workflows

from qatp_core.qatp.tasks import QATPSuite

class Chance(QATPSuite):
    _name = 'chance'

    def __init__(self):
        self.tasks = chance_tasks.get_tasks()
        self.workflows = chance_workflows.get_workflows()

    def get_name(self):
        return Chance._name
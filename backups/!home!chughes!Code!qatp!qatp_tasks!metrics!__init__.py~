from . import ingest_tasks, ingest_workflows

from qatp_core.qatp.tasks import QATPSuite


class Ingest(QATPSuite):
    NAME = 'ingest'

    def __init__(self):
        self.tasks = ingest_tasks.get_tasks()
        self.workflows = ingest_workflows.get_workflows()

    def get_name(self):
        return Ingest.NAME

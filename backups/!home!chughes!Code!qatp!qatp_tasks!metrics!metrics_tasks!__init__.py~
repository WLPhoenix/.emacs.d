from .configure_secure import ConfigureSecure
from .configure_streaming import ConfigureStreaming
from .configure_synthesys import ConfigureSynthesys
from .create_kb import CreateKB
from .fetch_dataset import FetchDataset
from .install_synthesys import InstallSynthesys
from .load_dataset import LoadDataset
from .run_associativenet import RunAssociativenet
from .run_frequencies import RunFrequencies
from .run_knowledgeobjects import RunKnowledgeObjects
from .run_queries import RunQueries
from .run_resonance import RunResonance
from .run_rivulet import RunRivulet
from .run_schedule import RunSchedule
from .start_synic import StartSynic
from .start_synthesysserver import StartSynthesysServer
from .stream_rivulet import StreamRivulet


def get_tasks():
    return {
        ConfigureSecure.NAME: ConfigureSecure,
        ConfigureStreaming.NAME: ConfigureStreaming,
        ConfigureSynthesys.NAME: ConfigureSynthesys,
        CreateKB.NAME: CreateKB,
        FetchDataset.NAME: FetchDataset,
        InstallSynthesys.NAME: InstallSynthesys,
        LoadDataset.NAME: LoadDataset,
        RunAssociativenet.NAME: RunAssociativenet,
        RunFrequencies.NAME: RunFrequencies,
        RunKnowledgeObjects.NAME: RunKnowledgeObjects,
        RunQueries.NAME: RunQueries,
        RunResonance.NAME: RunResonance,
        RunRivulet.NAME: RunRivulet,
        RunSchedule.NAME: RunSchedule,
        StartSynic.NAME: StartSynic,
        StartSynthesysServer.NAME: StartSynthesysServer,
        StreamRivulet.NAME: StreamRivulet,
    }


def get_task(name):
    return get_tasks().get(name)
